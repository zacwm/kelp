  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch server.ts",
      "runtimeArgs": [
        "--inspect",
        "-r",
        "ts-node/register"
      ],
      "args": [
        "${workspaceFolder}/src/server.ts",
      ],
      "sourceMaps": true
    },
    {
      "name": "Attach",
      "type": "node",
      "request": "attach",
      "port": 5858
    }
  ]
}